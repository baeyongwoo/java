interface Predator{
	//추상메서드 {}이 없음 선언만 함
	String getFood();
}

class Tiger implements Predator{

	//메서드 오버라이딩. 부모의 추상메서드를 반드시 오버라이딩 해야함
	@Override
	public String getFood() {
		return "apple";
	}
}


public class InterfaceTest {

	static int test = 1;
	public static void main(String[] args) {
		
	}
}
