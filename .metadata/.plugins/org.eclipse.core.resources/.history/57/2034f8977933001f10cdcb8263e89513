class Animal{
	String name;
	void setName(String name) {
		this.name = name;
	}
}

class Dog extends Animal{
	void sleep() {
		System.out.println(this.name + "가 zzz");
	}
}




public class Sample4 {
	public static void main(String[] args) {
		Dog dog = new Dog();
		dog.setName("찰롱");
		dog.sleep();
	}
}
