class Student{
	
	private String name;
	
	String getName(Student student) {
		return name = this.name;
	}
	
	void setName(String name) {
		name = this.name;
	}
	
	
}



public class ReferenceTest {
	public static void main(String[] args) {
		Student std1 = new Student();
		std1.setName("홍길동");
		
		System.out.println(std1.getName("홍길동"));
		
		
	}
}
