class Updator{
	void updator(Count count) {
		count.count++;
	}
}

class Count{
	int count = 0;
}


public class Sample2 {

	int a;
	int varTest(int a) {
		this.a = a;
		
		return a;
	}
	
	
	
	
	public static void main(String[] args) {
		int a =1;
		
		Sample2 sample = new Sample2();
		a = sample.varTest(a);
		System.out.println(a);
	}
}
